.uni-uploader {
  padding: 10px;
}
.uni-uploader .uni-uploader-head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.uni-uploader .uni-uploader-head .uni-uploader-title {
  font-weight: 545;
  font-size: 16px;
}
.uni-uploader .uni-uploader-head .uni-uploader-info {
  color: #999;
  padding-right: 14px;
}
.uni-uploader .uni-uploader__file {
  padding-top: 30px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.uni-uploader .uni-uploader__file .uni-uploader-file-img .item {
  width: 100px;
  height: 100px;
  padding: 6px;
}
.uni-uploader .uni-uploader__file .a {
  width: 100px;
  height: 100px;
  background-color: #999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.uni-uploader .label {
  padding-top: 30px;
}
.uni-uploader .label .label_title {
  padding: 10px 0;
  border-top: 1px solid #eee;
  font-size: 16px;
}
.uni-uploader .label .lable_items {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.uni-uploader .label .lable_items .lable_item {
  padding: 3px 5px;
  border: 1px solid #eee;
  -webkit-border-radius: 10px;
          border-radius: 10px;
  margin-right: 5px;
  -webkit-box-shadow: 0 0 5px 1px rgba(0, 0, 0, 0.1);
          box-shadow: 0 0 5px 1px rgba(0, 0, 0, 0.1);
}
.uni-uploader .label .lable_items .lable_itemp {
  background-color: #FEDE33;
  color: #fff;
}

