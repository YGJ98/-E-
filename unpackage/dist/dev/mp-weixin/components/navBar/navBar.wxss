.navbar {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.navbar .nav-fixed {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #FEDE33;
  z-index: 999;
}
.navbar .nav-fixed .nav-gation {
  padding: 0 5px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navbar .nav-fixed .nav-gation .nav-search {
  -webkit-border-radius: 10px;
          border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 5px;
  width: 100%;
  background-color: #F7F7F7;
}
.navbar .nav-fixed .nav-gation .nav-search .iconS {
  color: #999;
  font-size: 18px;
}
.navbar .nav-fixed .nav-gation .nav-search input {
  padding-left: 5px;
}
.navbar .nav-fixed .nav-gation .home-search {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
.navbar .nav-fixed .nav-gation .home-search .nav-search {
  -webkit-box-flex: 4;
  -webkit-flex: 4;
          flex: 4;
}
.navbar .nav-fixed .nav-gation .home-search .back {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-left: 10px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 16px;
  font-weight: 500;
}

