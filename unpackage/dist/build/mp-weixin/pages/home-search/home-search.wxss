page{height:100%;background-color:#f5f5f5}page .home{padding:10px;background-color:#fff}page .home .home-title{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between;font-size:16px;font-weight:500;border-bottom:1px solid #eee;padding-bottom:10px}page .home .home-title .clear{color:#30b33a}page .home .history-search{padding:10px 0;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap}page .home .history-search .item{border:1px solid #eee;padding:3px 5px;-webkit-border-radius:10px;border-radius:10px;margin:5px}page .home .noData{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;color:#999;padding:20px 0;font-size:16px}page .scroll{height:100%;background-color:#fff}page .scroll .no-search{height:100%;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-sizing:border-box;box-sizing:border-box}page .scroll .no-search .a{position:absolute;top:150px;text-align:center}page .scroll .no-search .a > image{width:200px;height:200px}