<view class="uni-uploader"><view class="uni-uploader-head"><view class="uni-uploader-title">点击可预览选好的图片</view><view class="uni-uploader-info">{{updateList.length+"/9"}}</view></view><view class="uni-uploader__file"><block wx:for="{{updateList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-uploader-file-img"><image class="item" src="{{item}}" data-event-opts="{{[['tap',[['previewImg',[index]]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{updateVideo.length>0}}"><view class="uni-uploader-file-video"><video src="{{updateVideo[0].tempFilePath}}" controls="{{true}}"></video></view></block><block wx:if="{{updateList.length<9&&updateVideo.length<1}}"><view data-event-opts="{{[['tap',[['update',['$event']]]]]}}" class="a" bindtap="__e"><uni-icons vue-id="28064722-1" type="plusempty" size="32" bind:__l="__l"></uni-icons></view></block></view><view class="label"><view class="label_title">标签</view><view class="lable_items"><block wx:for="{{lableList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickLable',[index]]]]]}}" class="{{['lable_item',(tabIndex===index)?'lable_itemp':'']}}" bindtap="__e">{{item.name}}</view></block></view></view></view>